{
	"name": "coldfusion",
	"displayName": "ColdFusion",
	"description": "CFML (ColdFusion) Language Support",
	"version": "0.0.8",
	"publisher": "ilich8086",
	"homepage": "https://github.com/ilich/vscode-coldfusion",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/ilich/vscode-coldfusion.git"
	},
	"bugs": {
		"url": "https://github.com/ilich/vscode-coldfusion/issues"
	},
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Languages"
	],
	"contributes": {
		"languages": [
			{ "id": "lang-cfml", "aliases": ["CFML"], "extensions": [".cfm",".cfml",".cfc"], "configuration": "./cfscript.configuration.json"}
		],
		"grammars": [
			{"language": "lang-cfml", "scopeName": "embedding.cfml", "path": "./syntaxes/cfml.tmLanguage" }
		],
		"snippets": [
			{
				"language": "lang-cfml",
				"path": "./snippets/snippets.json"
			}
		]
	}
}